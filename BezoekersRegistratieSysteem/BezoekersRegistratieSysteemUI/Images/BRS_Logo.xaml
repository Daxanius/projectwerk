<UserControl
    x:Class="BezoekersRegistratieSysteemUI.Images.BRS_Logo"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:BezoekersRegistratieSysteemUI.Images"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    FontFamily="{StaticResource Inter}"
    mc:Ignorable="d">
    <Viewbox>
        <Canvas Width="645" Height="500">
            <Ellipse
                Canvas.Left="110"
                Width="500"
                Height="500"
                Fill="{StaticResource MainAchtergrond}" />
            <Ellipse
                Canvas.Top="235"
                Width="70"
                Height="70"
                Fill="{StaticResource GewoonBlauw}" />
            <Label
                Canvas.Left="70"
                Canvas.Top="105"
                Content="inside"
                FontSize="203"
                FontWeight="Medium"
                Foreground="{StaticResource DonkerBlauw}" />
        </Canvas>
    </Viewbox>
</UserControl>
