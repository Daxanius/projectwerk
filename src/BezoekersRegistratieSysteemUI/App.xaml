<Application
    x:Class="BezoekersRegistratieSysteemUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    DispatcherUnhandledException="VangOntsnapteExceptions"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="Inter">pack://application:,,,/Fonts/#Inter</FontFamily>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <FontFamily x:Key="Inter">/Fonts/#Inter</FontFamily>
                </ResourceDictionary>
                <ResourceDictionary Source="\Style\Style.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>